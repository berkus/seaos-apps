.TH tan 3 "" "" ""
.SH SYNOPSIS
tan, tanf \- tangent
.SH ANSI_SYNOPSIS
#include <math.h>
.br
double tan(double 
.IR x );
.br
float tanf(float 
.IR x );
.br
.SH TRAD_SYNOPSIS
#include <math.h>
.br
double tan(
.IR x )
.br
double 
.IR x ;
.br

float tanf(
.IR x )
.br
float 
